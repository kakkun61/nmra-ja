// spell-checker: disable
{
  "version": "0.2",
  "language": "en",
  "words": [
    "autosummary",
    "fleischmann",
    "intersphinx",
    "kazuki",
    "nixos",
    "nixpkgs",
    "nmra",
    "nvram",
    "okamoto",
    "pkgs",
    "railcommand",
    "subaddress",
    "subinstruction",
    "susi",
    "tabularized",
    "toctree",
    "trix",
    "wayback",
    "μsec",
  ],
  "patterns": [
    {
      "name": "LaTeX preamble",
      "pattern": "\\\\mathrm\\{\\w+\\}"
    },
    // 中途半端にマッチして、正規の単語が分割されてエラーになるので、正規表現ではなく出現した文字列を列挙する
    {
      "name": "Format Definition",
      "pattern": "CCCD|SDDDD|DLLLLLLL|CCVV|CDDD|DCSSSS|CSSSS"
    }
  ],
  "ignoreRegExpList": [
    "LaTeX preamble",
    "Format Definition"
  ],
  "ignorePaths": [
    ".direnv",
    ".github",
    ".vscode",
    ".editorconfig",
    ".envrc",
    ".gitignore",
    ".readthedocs.yaml",
    "cspell.json",
    "LICENSE",
    "Makefile",
    "package-lock.json",
    "hon"
  ]
}
